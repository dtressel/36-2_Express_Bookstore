{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
      "isbn",
      "amazon_url",
      "author",
      "language",
      "pages",
      "publisher",
      "title",
      "year"
  ],
  "properties": {
      "isbn": {
          "type": "string",
          "default": "",
          "title": "The isbn Schema",
          "examples": [
              "9781484249666"
          ]
      },
      "amazon_url": {
          "type": "string",
          "default": "",
          "title": "The amazon_url Schema",
          "examples": [
              "https://a.co/d/5MYO6uT"
          ]
      },
      "author": {
          "type": "string",
          "default": "",
          "title": "The author Schema",
          "examples": [
              "Francesco Strazzullo"
          ]
      },
      "language": {
          "type": "string",
          "default": "",
          "title": "The language Schema",
          "examples": [
              "English"
          ]
      },
      "pages": {
          "type": "integer",
          "default": 0,
          "title": "The pages Schema",
          "examples": [
              265
          ]
      },
      "publisher": {
          "type": "string",
          "default": "",
          "title": "The publisher Schema",
          "examples": [
              "Apress"
          ]
      },
      "title": {
          "type": "string",
          "default": "",
          "title": "The title Schema",
          "examples": [
              "Frameworkless Front-End Development"
          ]
      },
      "year": {
          "type": "integer",
          "default": 0,
          "title": "The year Schema",
          "examples": [
              2019
          ]
      }
  },
  "examples": [{
      "isbn": 9781484249666,
      "amazon_url": "https://a.co/d/5MYO6uT",
      "author": "Francesco Strazzullo",
      "language": "English",
      "pages": 265,
      "publisher": "Apress",
      "title": "Frameworkless Front-End Development",
      "year": 2019
  }]
}